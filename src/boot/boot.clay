import display;
import multiboot.*;

external kmain(multiboot:Pointer[MultibootInfo], magic:UInt32) {
	display.clear();
	display.println("Hello, ", "world", '!');
	display.currentColor = display.color(display.WHITE, display.RED);
	display.println("Blah blah blah.");
	display.currentColor = display.DEFAULT_COLOR;
}
